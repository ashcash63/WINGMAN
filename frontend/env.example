# Beeper Desktop API Configuration
# Copy this file to .env.local and fill in your actual access token

# Your Beeper Desktop API access token
# Get this from: Beeper Desktop → Settings → Advanced → Desktop API
NEXT_PUBLIC_BEEPER_ACCESS_TOKEN=your_access_token_here

# Cohere API Configuration
# Your Cohere API key for AI features (both client and server-side)
NEXT_PUBLIC_COHERE_API_KEY=your_cohere_api_key_here
COHERE_API_KEY=your_cohere_api_key_here

# VAPI Configuration
# Your VAPI API key for voice assistant integration
VAPI_API_KEY=your_vapi_api_key_here

# Your VAPI assistant ID (created in VAPI dashboard)
VAPI_ASSISTANT_ID=your_assistant_id_here

# Webhook URL for VAPI callbacks (your deployed app URL + /api/vapi-webhook)
VAPI_WEBHOOK_URL=https://your-app-url.com/api/vapi-webhook
